{
  "expo": {
    "name": "Mushaf",
    "slug": "open-mushaf-native",
    "version": "1.0.14",
    "orientation": "default",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adelpro.openmushafnative"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adelpro.openmushafnative",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.adelpro.openmushafnative",
      "enableProguardInReleaseBuilds": true,
      "jsEngine": "hermes",
      "permissions": ["Aw"],
      "softwareKeyboardLayoutMode": "pan",
      "buildProperties": {
        "android": {
          "enableShrinkResourcesInReleaseBuilds": true,
          "enableR8": true
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-av",
      "expo-secure-store",
      "expo-updates"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "eas": {
        "projectId": "2360a464-857f-4685-8c0f-be166a166847"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/2360a464-857f-4685-8c0f-be166a166847",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "owner": "adelpro"
  }
}
